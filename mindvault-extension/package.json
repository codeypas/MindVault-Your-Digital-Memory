{
  "name": "mindvault-extension",
  "version": "1.0.0",
  "description": "MindVault Chrome Extension",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && npm run copy-files",
    "copy-files": "cp src/manifest.json dist/ && cp -r src/icons dist/ 2>/dev/null || true"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "jspdf": "^3.0.1"
  }
}
